{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/game/*": ["src/game/*"],
      "@/stores/*": ["src/stores/*"],
  "@/utils/*": ["src/utils/*"],
  "@/net/*": ["src/net/*"]
    }
  },
  "include": ["src"]
}
